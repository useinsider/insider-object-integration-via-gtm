const insiderObject = copyFromWindow('insider_object') || {};
const dataLayer = copyFromWindow('dataLayer');
const eventType = data.eventType;

var getNestedData = function (parentData, dataFieldPath) {
if (typeof dataFieldPath === 'string') {
dataFieldPath = dataFieldPath.split('.');
}

if (typeof parentData !== 'undefined') {
dataFieldPath.forEach(function (field) {
parentData = parentData[field];
});
}

return parentData;
};

var getData = function (data) {
var dataFieldPath = data.split('.');
var parentDataName = dataFieldPath[0];
var parentData;
dataFieldPath.shift();
dataLayer.some(function (windowData) {
if (typeof windowData[parentDataName] !== 'undefined') {
parentData = windowData[parentDataName];
return true;
}
});

return getNestedData(parentData, dataFieldPath);
};

var getDataFromDataLayer = function (data) {
var dataFieldPath = data.split('.');
var parentDataName = dataFieldPath[0];
var parentData = {};

dataFieldPath.shift();

dataLayer.some(function (windowData) {
if (typeof windowData[parentDataName] !== 'undefined') {
parentData = windowData[parentDataName];
return true;
}
});

if (typeof parentData !== 'undefined') {
dataFieldPath.forEach(function (field) {
parentData = parentData[field];
});
}

return parentData;
};

